{
  "name": "electron-react-boilerplate",
  "version": "0.4.3",
  "description": "Electron application boilerplate based on React, React Router, Webpack, React Hot Loader for rapid application development",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "dev-server": "webpack --config ./webpack.config.production.js --progress --colors",
    "hot-dev-server": "webpack-dev-server --config ./webpack.config.js --hot --progress --colors --port 2992 --inline",
    "build": "webpack --config ./webpack.config.production.js --progress --profile --colors",
    "start": "electron .",
    "start-dev": "NODE_ENV=development electron .",
    "start-hot": "HOT=1 NODE_ENV=development electron .",
    "package": "node package.js",
    "package-all": "node package.js --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chentsulin/electron-react-boilerplate.git"
  },
  "author": {
